type Query {
    "Returns configuration."
    configuration: Configuration

    "Returns all the strategies."
    strategies: [Strategy]
    "Returns the strategy with the corresponding id value."
    strategy(id: Int): Strategy
    "Returns the strategy with the corresponding strategyId value."
    strategyByStrategyId(strategyId: String): Strategy

    "Returns all the orders."
    orders: [Order]
    "Returns the order with the corresponding id value."
    order(id: Int): Order
    "Returns the order with the corresponding orderId value."
    orderByOrderId(orderId: String): Order

    "Returns all the trades."
    trades: [Trade]
    "Returns the trade with the corresponding id value."
    trade(id: Int): Trade
    "Returns the trade with the corresponding tradeId value."
    tradeByTradeId(tradeId: String): Trade

}

scalar DateTime